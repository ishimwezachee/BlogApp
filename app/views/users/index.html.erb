<% @users.each do |user| %>
      <div class="user_img">
      <img src=<%='https://cdn.pixabay.com/photo/2018/08/28/12/41/avatar-3637425__340.png' %> alt="profile picture">
      </div>
      <div class="user-data">
        <%= link_to user_path(user.id) do %>
        <h2><%= user.name%></h2>
        <% end %>
        <p>Number of posts: <%= user.post_ids.count %></p>
      </div>
  <%end%>
